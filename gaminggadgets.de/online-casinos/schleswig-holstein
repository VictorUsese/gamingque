    function togglePayments(brand) {
        var x = document.querySelector(".payments-" + brand + "");

        if (!x.classList.contains('show')) {
            x.classList.add('show');

            document.getElementById(brand + "-payments-more").remove();
        } else {
            x.classList.remove('show');
        }
    }

window.addEventListener('load', (event) => {
    jQuery('body').on('click', '.quickftc', function(event) {
        event.preventDefault();
        jQuery(this).parent().parent().parent().parent().parent().find(
            ".quickfacts-1").toggleClass('activefact');
    });

    function getSorted(selector, attrName) {
        return jQuery(jQuery(selector).toArray().sort(function(a, b) {
            var aVal = parseInt(a.getAttribute(attrName)),
                bVal = parseInt(b.getAttribute(attrName));
            return bVal - aVal;
        }));
    }

    function getdSorted(selector, attrName) {
        return jQuery(jQuery(selector).toArray().sort(function(a, b) {
            var aVal = parseInt(a.getAttribute(attrName)),
                bVal = parseInt(b.getAttribute(attrName));
            return aVal - bVal;
        }));
    }
    jQuery(".bestbonussort-1").on("click", function(event) {
        event.preventDefault();
        jQuery(".sortingbtn").removeClass('active');
        jQuery(this).addClass('active');
        var htmlsortedblock = getSorted(".toplistrow-1", 'data-bonus');
        jQuery(".tablistcontainer-1").html(htmlsortedblock);
        reinitToogle();
    });
    jQuery(".bestfreespin-1").on("click", function(event) {
        event.preventDefault();
        jQuery(".sortingbtn").removeClass('active');
        jQuery(this).addClass('active');
        var htmlsortedblock = (getSorted(".toplistrow-1", 'data-free-spin'));
        jQuery(".tablistcontainer-1").html(htmlsortedblock);
        reinitToogle();
    });
    jQuery(".recentlyadded-1").on("click", function(event) {
        event.preventDefault();
        jQuery(".sortingbtn").removeClass('active');
        jQuery(this).addClass('active');
        var htmlsortedblock = (getSorted(".toplistrow-1", 'data-date'));
        jQuery(".tablistcontainer-1").html(htmlsortedblock);
        reinitToogle();
    });
    jQuery(".bestrating-1").on("click", function(event) {
        event.preventDefault();
        jQuery(".sortingbtn").removeClass('active');
        jQuery(this).addClass('active');
        var htmlsortedblock = getdSorted(".toplistrow-1", 'data-sort');
        jQuery(".tablistcontainer-1").html(htmlsortedblock);
        reinitToogle();
    });

    jQuery(".tabmobile").find('.qtop2').on("click", function(event) {
        event.preventDefault();
        jQuery(this).parent().find(".disptabcontent").eq(0).toggle();
        jQuery(this).find(".fa-chevron-right").eq(0).toggleClass("rotate90");
    });

    jQuery(".tabpayment").on("click", function(event) {
