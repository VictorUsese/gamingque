<script type="rocketlazyloadscript">
    function togglePayments(brand) {
        var x = document.querySelector(".payments-" + brand + "");

        if (!x.classList.contains('show')) {
            x.classList.add('show');

            document.getElementById(brand + "-payments-more").remove();
        } else {
            x.classList.remove('show');
        }
    }

window.addEventListener('load', (event) => {
    jQuery('body').on('click', '.quickftc', function(event) {
        event.preventDefault();
        jQuery(this).parent().parent().parent().parent().parent().find(
            ".quickfacts-2").toggleClass('activefact');
    });

    function getSorted(selector, attrName) {
        return jQuery(jQuery(selector).toArray().sort(function(a, b) {
            var aVal = parseInt(a.getAttribute(attrName)),
                bVal = parseInt(b.getAttribute(attrName));
            return bVal - aVal;
        }));
    }

    function getdSorted(selector, attrName) {
        return jQuery(jQuery(selector).toArray().sort(function(a, b) {
            var aVal = parseInt(a.getAttribute(attrName)),
                bVal = parseInt(b.getAttribute(attrName));
            return aVal - bVal;
        }));
    }
    jQuery(".bestbonussort-2").on("click", function(event) {
        event.preventDefault();
        jQuery(".sortingbtn").removeClass('active');
        jQuery(this).addClass('active');
        var htmlsortedblock = getSorted(".toplistrow-2", 'data-bonus');
        jQuery(".tablistcontainer-2").html(htmlsortedblock);
        reinitToogle();
    });
    jQuery(".bestfreespin-2").on("click", function(event) {
        event.preventDefault();
        jQuery(".sortingbtn").removeClass('active');
        jQuery(this).addClass('active');
        var htmlsortedblock = (getSorted(".toplistrow-2", 'data-free-spin'));
        jQuery(".tablistcontainer-2").html(htmlsortedblock);
        reinitToogle();
    });
    jQuery(".recentlyadded-2").on("click", function(event) {
        event.preventDefault();
        jQuery(".sortingbtn").removeClass('active');
        jQuery(this).addClass('active');
        var htmlsortedblock = (getSorted(".toplistrow-2", 'data-date'));
        jQuery(".tablistcontainer-2").html(htmlsortedblock);
        reinitToogle();
    });
    jQuery(".bestrating-2").on("click", function(event) {
        event.preventDefault();
        jQuery(".sortingbtn").removeClass('active');
        jQuery(this).addClass('active');
        var htmlsortedblock = getdSorted(".toplistrow-2", 'data-sort');
        jQuery(".tablistcontainer-2").html(htmlsortedblock);
        reinitToogle();
    });

    jQuery(".tabmobile").find('.qtop2').on("click", function(event) {
        event.preventDefault();
        jQuery(this).parent().find(".disptabcontent").eq(0).toggle();
        jQuery(this).find(".fa-chevron-right").eq(0).toggleClass("rotate90");
    });

    jQuery(".tabpayment").on("click", function(event) {
        event.preventDefault();
        jQuery(this).parent().find(".disptabcontentadd").toggle();
        jQuery(this).find(".fa-chevron-right").eq(0).toggleClass("rotate90");
    });

    jQuery(".tabgames").on("click", function(event) {
        event.preventDefault();
        jQuery(this).parent().find(".disptabcontentadd").toggle();
        jQuery(this).find(".fa-chevron-right").eq(0).toggleClass("rotate90");
    });

    jQuery('body').on('click', '.loadmoregame', function(event) {
        event.preventDefault();

        jQuery(this).parent().find(".list_gameprovider").find(".hidedefault").addClass("showdefault")
            .removeClass("hidedefault");
        jQuery(this).hide();
    });

    jQuery('body').on('click', '.loadmorepay', function(event) {
        event.preventDefault();

        jQuery(this).parent().find(".list_paymentprovider").find(".hidedefault").addClass("showdefault")
            .removeClass("hidedefault");
        jQuery(this).hide();
    });

    function reinitToogle() {
        jQuery(".tabmobile").find('.qtop2').on("click", function(event) {
            event.preventDefault();
            jQuery(this).parent().find(".disptabcontent").eq(0).toggle();
            jQuery(this).find(".fa-chevron-right").eq(0).toggleClass("rotate90");
        });

        jQuery(".tabpayment").on("click", function(event) {
            event.preventDefault();
            jQuery(this).parent().find(".disptabcontentadd").toggle();
            jQuery(this).find(".fa-chevron-right").eq(0).toggleClass("rotate90");
        });

    }

    function hideByText2(text, opts, toplistrowclass) {
        if (!text) {
            jQuery(".toplistrow-2").show();
            return false;
        } else {
            jQuery(".toplistrow-2").show();
            var defaults = {
                    // the element we look within, expects:
                    // 1: node reference, eg the result of: document.getElementsByTagName('div')[0]
                    // 2: an element's id, as a string, eg: 'test'
                    'within': "toplistrow-2",
                    // the element type, eg 'div', 'span', 'p', defaults to *everything*
                    'elemType': '*',
                    // case-sensitivity, as a string:
                    // 'true' : is case sensitive, 'Some' will not match 'some',
                    // 'false' : is case insensitive, 'Some' will match 'some'
                    'sensitive': 'true',
                    // 'absolute' : 'some text' will not match 'some text.'
                    // 'partial' : 'some text' will match 'some text.'
                    'match': 'absolute',
                    // 'true' : removes white-space from beginning, and end, of the text,
                    // 'false' : does not remove white-space
                    'trim': 'true',
                    // the class to add to elements if a match is made,
                    // use CSS to hide, or style, the matched elements
                    'matchedClass': 'hasText'
                },
                opts = opts || {};
            for (var setting in defaults) {
                if (defaults.hasOwnProperty(setting)) {
                    opts[setting] = opts[setting] || defaults[setting];
                }
            }
            var within = opts.within.nodeType == 1 ? opts.within : document.getElementById(opts.within),
                elems = jQuery(".toplistrow-2"),
                flags = opts.sensitive == 'true' ? 'i' : '',
                needle = opts.trim == 'true' ? text.replace(/^(\s+) || (\s+)$/g, '') : text,
                haystack,
                reg = new RegExp(needle, flags);
            if (opts.match == 'absolute') {
                for (var i = 0, len = elems.length; i < len; i++) {
                    if ((elems[i].textContent || elems[i].innerText) == text) {
                        elems[i].className = opts.matchedClass;
                    }
                }
            } else if (opts.match == 'partial') {
                jQuery(".toplistrow-2").show();
                for (var i = 0, len = elems.length; i < len; i++) {
                    //alert(jQuery("."+elems[i].className).attr("data-casino-title"));
                    //alert(jQuery("."+elems[i].className).find(".casino_name").text());
                    //alert(reg);
                    if (elems[i].getAttribute("data-casino-title").match(reg)) {
                        // elems[i].className = opts.matchedClass;
                        //elems[i].className;
                        jQuery("#" + elems[i].id).show();
                    } else {
                        jQuery("#" + elems[i].id).hide();
                    }
                }
            }
        }
    }
    jQuery("#searchInput-2").keyup(function() {
        hideByText2(jQuery(this).val(), {
                'match': 'partial',
                'sensitive': 'true',
                'elemType': 'div',
                'matchedClass': 'disprow'
            },
            "toplistrow-2"
        )
    });

}, {
    passive: true
});
</script>





