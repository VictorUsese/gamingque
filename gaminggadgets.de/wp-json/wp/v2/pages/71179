        \".quickfacts-2\").toggleClass('activefact');\n    });\n\n    function getSorted(selector, attrName) {\n        return jQuery(jQuery(selector).toArray().sort(function(a, b) {\n            var aVal = parseInt(a.getAttribute(attrName)),\n                bVal = parseInt(b.getAttribute(attrName));\n            return bVal - aVal;\n        }));\n    }\n\n    function getdSorted(selector, attrName) {\n        return jQuery(jQuery(selector).toArray().sort(function(a, b) {\n            var aVal = parseInt(a.getAttribute(attrName)),\n                bVal = parseInt(b.getAttribute(attrName));\n            return aVal - bVal;\n        }));\n    }\n    jQuery(\".bestbonussort-2\").on(\"click\", function(event) {\n        event.preventDefault();\n        jQuery(\".sortingbtn\").removeClass('active');\n        jQuery(this).addClass('active');\n        var htmlsortedblock = getSorted(\".toplistrow-2\", 'data-bonus');\n        jQuery(\".tablistcontainer-2\").html(htmlsortedblock);\n        reinitToogle();\n    });\n    jQuery(\".bestfreespin-2\").on(\"click\", function(event) {\n        event.preventDefault();\n        jQuery(\".sortingbtn\").removeClass('active');\n        jQuery(this).addClass('active');\n        var htmlsortedblock = (getSorted(\".toplistrow-2\", 'data-free-spin'));\n        jQuery(\".tablistcontainer-2\").html(htmlsortedblock);\n        reinitToogle();\n    });\n    jQuery(\".recentlyadded-2\").on(\"click\", function(event) {\n        event.preventDefault();\n        jQuery(\".sortingbtn\").removeClass('active');\n        jQuery(this).addClass('active');\n        var htmlsortedblock = (getSorted(\".toplistrow-2\", 'data-date'));\n        jQuery(\".tablistcontainer-2\").html(htmlsortedblock);\n        reinitToogle();\n    });\n    jQuery(\".bestrating-2\").on(\"click\", function(event) {\n        event.preventDefault();\n        jQuery(\".sortingbtn\").removeClass('active');\n        jQuery(this).addClass('active');\n        var htmlsortedblock = getdSor