ents\n                    'matchedClass': 'hasText'\n                },\n                opts = opts || {};\n            for (var setting in defaults) {\n                if (defaults.hasOwnProperty(setting)) {\n                    opts[setting] = opts[setting] || defaults[setting];\n                }\n            }\n            var within = opts.within.nodeType == 1 ? opts.within : document.getElementById(opts.within),\n                elems = jQuery(\".toplistrow-3\"),\n                flags = opts.sensitive == 'true' ? 'i' : '',\n                needle = opts.trim == 'true' ? text.replace(\/^(\\s+) || (\\s+)$\/g, '') : text,\n                haystack,\n                reg = new RegExp(needle, flags);\n            if (opts.match == 'absolute') {\n                for (var i = 0, len = elems.length; i < len; i++) {\n                    if ((elems[i].textContent || elems[i].innerText) == text) {\n                        elems[i].className = opts.matchedClass;\n                    }\n                }\n            } else if (opts.match == 'partial') {\n                jQuery(\".toplistrow-3\").show();\n                for (var i = 0, len = elems.length; i < len; i++) {\n                    \/\/alert(jQuery(\".\"+elems[i].className).attr(\"data-casino-title\"));\n                    \/\/alert(jQuery(\".\"+elems[i].className).find(\".casino_name\").text());\n                    \/\/alert(reg);\n                    if (elems[i].getAttribute(\"data-casino-title\").match(reg)) {\n                        \/\/ elems[i].className = opts.matchedClass;\n                        \/\/elems[i].className;\n                        jQuery(\"#\" + elems[i].id).show();\n                    } else {\n                        jQuery(\"#\" + elems[i].id).hide();\n                    }\n                }\n            }\n        }\n    }\n    jQuery(\"#searchInput-3\").keyup(function() {\n        hideByText3(jQuery(this).val(), {\n                'match': 'partial',\n                'sensitive': 'true',\n                'elemType': 'div',\n                'matchedClass': 'disprow'\n            },\n            \"toplistrow-3\"\n        )\n    });\n\n}, {\n    passive: true\n});\n<\/script>\n\n\n\n\n\n<hr class=\"wp-block-separator has-alpha-channel-opacity\"\/>\n\n\n\n<h3 id=\"h-1bet\">1bet<\/h3>\n\n\n<div class=\"wp-block-image\">\n<figure class=\"aligncenter size-large is-resized\"><img decoding=\"async\" src=\"https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/1bet-startseite-1024x637.jpg\" alt=\"\" class=\"wp-image-70716\" width=\"790\" srcset=\"https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/1bet-startseite-1024x637.jpg 1024w, https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/1bet-startseite-300x187.jpg 300w, https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/1bet-startseite-768x478.jpg 768w, https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/1bet-startseite.jpg 1280w\" sizes=\"(max-width: 1024px) 100vw, 1024px\" \/><figcaption class=\"wp-element-caption\"><em>1bet Startseite<\/em><\/figcaption><\/figure><\/div>\n\n\n<ul>\n<li>1bet verf\u00fcgt \u00fcber eine sichere Lizenz der Regulierungsbeh\u00f6rde aus Cura\u00e7ao<\/li>\n\n\n\n<li>Als Neukunden kassiert ihr 100% Bonus bis zu 100\u20ac f\u00fcr eure erste Einzahlung<\/li>\n\n\n\n<li>Zahlreiche Specials und Aktionen stehen f\u00fcr Bestandskunden zur Verf\u00fcgung<\/li>\n\n\n\n<li>Jedes Wochenende k\u00f6nnt ihr bei 1bet Gratiswetten kassieren<\/li>\n<\/ul>\n\n\n\n<link rel='stylesheet' id='my_css-css' href='\/wp-content\/plugins\/hpmtable\/css\/style.css' type='text\/css' media='all' \/>\n\n<style>\n.brand {\n    position: relative;\n}\n\n.brand img {\n    width: 200px;\n    height: 80px;\n}\n\n.casino_field_1 {\n    position: absolute;\n    left: 5px;\n    background: rgba(252, 215, 48, 0.9);\n    padding: 1px 12px;\n    top: -10px;\n    font-size: 12px;\n    font-weight: 700;\n}\n\n.casino_field_2.ribbonm {\n    padding: 5px 20px;\n    border: 1px so