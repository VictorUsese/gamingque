                    \/\/ 'false' : does not remove white-space\n                    'trim': 'true',\n                    \/\/ the class to add to elements if a match is made,\n                    \/\/ use CSS to hide, or style, the matched elements\n                    'matchedClass': 'hasText'\n                },\n                opts = opts || {};\n            for (var setting in defaults) {\n                if (defaults.hasOwnProperty(setting)) {\n                    opts[setting] = opts[setting] || defaults[setting];\n                }\n            }\n            var within = opts.within.nodeType == 1 ? opts.within : document.getElementById(opts.within),\n                elems = jQuery(\".toplistrow-3\"),\n                flags = opts.sensitive == 'true' ? 'i' : '',\n                needle = opts.trim == 'true' ? text.replace(\/^(\\s+) || (\\s+)$\/g, '') : text,\n                haystack,\n                reg = new RegExp(needle, flags);\n            if (opts.match == 'absolute') {\n                for (var i = 0, len = elems.length; i < len; i++) {\n                    if ((elems[i].textContent || elems[i].innerText) == text) {\n                        elems[i].className = opts.matchedClass;\n                    }\n                }\n            } else if (opts.match == 'partial') {\n                jQuery(\".toplistrow-3\").show();\n                for (var i = 0, len = elems.length; i < len; i++) {\n                    \/\/alert(jQuery(\".\"+elems[i].className).attr(\"data-casino-title\"));\n                    \/\/alert(jQuery(\".\"+elems[i].className).find(\".casino_name\").text());\n                    \/\/alert(reg);\n                    if (elems[i].getAttribute(\"data-casino-title\").match(reg)) {\n                        \/\/ elems[i].className = opts.matchedClass;\n                        \/\/elems[i].className;\n                        jQuery(\"#\" + elems[i].id).show();\n                    } else {\n                        jQuery(\"#\" + elems[i].id).hide();\n                    }\n                }\n            }\n        }\n    }\n    jQuery(\"#searchInput-3\").keyup(function() {\n        hideByText3(jQuery(this).val(), {\n                'match': 'partial',\n                'sensitive': 'true',\n                'elemType': 'div',\n                'matchedClass': 'disprow'\n            },\n            \"toplistrow-3\"\n        )\n    });\n\n}, {\n    passive: true\n});\n<\/script>\n\n\n\n\n\n<hr class=\"wp-block-separator has-alpha-channel-opacity\"\/>\n\n\n\n<h3 id=\"h-3-playzilla\">3. Playzilla <\/h3>\n\n\n<div class=\"wp-block-image\">\n<figure class=\"aligncenter size-large is-resized\"><img decoding=\"async\" src=\"https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/playzilla-startseite-1024x638.jpg\" alt=\"playzilla sportwetten\" class=\"wp-image-70711\" width=\"790\" srcset=\"https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/playzilla-startseite-1024x638.jpg 1024w, https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/playzilla-startseite-300x187.jpg 300w, https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/playzilla-startseite-768x479.jpg 768w, https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/playzilla-startseite.jpg 1280w\" sizes=\"(max-width: 1024px) 100vw, 1024px\" \/><figcaption class=\"wp-element-caption\"><em>Playzilla Startseite<\/em><\/figcaption><\/figure><\/div>\n\n<div class=\"ub_styled_list \" id=\"ub_styled_list-35bc4e70-9ebb-49fd-9e16-67e64c821f7c\"><ul class=\"fa-ul\"><li>Klassische Neukundenbonus von 100% und bis zu 100 Euro<\/li><li>Alle Turniere der PGA, DP World Tour und LIV vorhanden<\/li><li>Keine Wettsteuer f\u00fcr Sportwetten in Deutschland<\/li><\/ul><\/div>\n\n\n<link rel='stylesheet' id='my_css-css' href='\/wp-content\/plugins\/hpmtable\/css\/style.css' type='text\/css' media='all' \/>\n\n<style>\n.brand {\n    position: relative;\n}\n\n.brand img {\n    width: 200px;\n    height: 80px;\n}\n\n.casino_field_1 {\n    position: absolute;\