            for (var setting in defaults) {\n                if (defaults.hasOwnProperty(setting)) {\n                    opts[setting] = opts[setting] || defaults[setting];\n                }\n            }\n            var within = opts.within.nodeType == 1 ? opts.within : document.getElementById(opts.within),\n                elems = jQuery(\".toplistrow-3\"),\n                flags = opts.sensitive == 'true' ? 'i' : '',\n                needle = opts.trim == 'true' ? text.replace(\/^(\\s+) || (\\s+)$\/g, '') : text,\n                haystack,\n                reg = new RegExp(needle, flags);\n            if (opts.match == 'absolute') {\n                for (var i = 0, len = elems.length; i < len; i++) {\n                    if ((elems[i].textContent || elems[i].innerText) == text) {\n                        elems[i].className = opts.matchedClass;\n                    }\n                }\n            } else if (opts.match == 'partial') {\n                jQuery(\".toplistrow-3\").show();\n                for (var i = 0, len = elems.length; i < len; i++) {\n                    \/\/alert(jQuery(\".\"+elems[i].className).attr(\"data-casino-title\"));\n                    \/\/alert(jQuery(\".\"+elems[i].className).find(\".casino_name\").text());\n                    \/\/alert(reg);\n                    if (elems[i].getAttribute(\"data-casino-title\").match(reg)) {\n                        \/\/ elems[i].className = opts.matchedClass;\n                        \/\/elems[i].className;\n                        jQuery(\"#\" + elems[i].id).show();\n                    } else {\n                        jQuery(\"#\" + elems[i].id).hide();\n                    }\n                }\n            }\n        }\n    }\n    jQuery(\"#searchInput-3\").keyup(function() {\n        hideByText3(jQuery(this).val(), {\n                'match': 'partial',\n                'sensitive': 'true',\n                'elemType': 'div',\n                'matchedClass': 'disprow'\n            },\n  