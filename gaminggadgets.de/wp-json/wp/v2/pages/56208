 'absolute',\n                    \/\/ 'true' : removes white-space from beginning, and end, of the text,\n                    \/\/ 'false' : does not remove white-space\n                    'trim': 'true',\n                    \/\/ the class to add to elements if a match is made,\n                    \/\/ use CSS to hide, or style, the matched elements\n                    'matchedClass': 'hasText'\n                },\n                opts = opts || {};\n            for (var setting in defaults) {\n                if (defaults.hasOwnProperty(setting)) {\n                    opts[setting] = opts[setting] || defaults[setting];\n                }\n            }\n            var within = opts.within.nodeType == 1 ? opts.within : document.getElementById(opts.within),\n                elems = jQuery(\".toplistrow-3\"),\n                flags = opts.sensitive == 'true' ? 'i' : '',\n                needle = opts.trim == 'true' ? text.replace(\/^(\\s+) || (\\s+)$\/g, '') : text,\n                haystack,\n                reg = new RegExp(needle, flags);\n            if (opts.match == 'absolute') {\n                for (var i = 0, len = elems.length; i < len; i++) {\n                    if ((elems[i].textContent || elems[i].innerText) == text) {\n                        elems[i].className = opts.matchedClass;\n                    }\n                }\n            } else if (opts.match == 'partial') {\n                jQuery(\".toplistrow-3\").show();\n                for (var i = 0, len = elems.length; i < len; i++) {\n                    \/\/alert(jQuery(\".\"+elems[i].className).attr(\"data-casino-title\"));\n                    \/\/alert(jQuery(\".\"+elems[i].className).find(\".casino_name\").text());\n                    \/\/alert(reg);\n                    if (elems[i].getAttribute(\"data-casino-title\").match(reg)) {\n                        \/\/ elems[i].className = opts.matchedClass;\n                        \/\/elems[i].className;\n                        jQuery(\"#\" + elems[i].id).show();\n                    } else {\n                        jQuery(\"#\" + elems[i].id).hide();\n                    }\n                }\n            }\n        }\n    }\n    jQuery(\"#searchInput-3\").keyup(function() {\n        hideByText3(jQuery(this).val(), {\n                'match': 'partial',\n                'sensitive': 'true',\n                'elemType': 'div',\n                'matchedClass': 'disprow'\n            },\n            \"toplistrow-3\"\n        )\n    });\n\n}, {\n    passive: true\n});\n<\/script>\n\n\n\n\n\n<hr class=\"wp-block-separator has-alpha-channel-opacity\"\/>\n\n\n\n<h3><strong>GGBet<\/strong><\/h3>\n\n\n<div class=\"wp-block-image\">\n<figure class=\"aligncenter size-full is-resized\"><img decoding=\"async\" src=\"https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/ggbet-sportwetten-webseite.jpg\" alt=\"ggbet sportwetten webseite\" class=\"wp-image-71013\" width=\"768\" height=\"374\" srcset=\"https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/ggbet-sportwetten-webseite.jpg 1024w, https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/ggbet-sportwetten-webseite-300x146.jpg 300w, https:\/\/gaminggadgets.de\/wp-content\/uploads\/2023\/02\/ggbet-sportwetten-webseite-768x374.jpg 768w\" sizes=\"(max-width: 768px) 100vw, 768px\" \/><\/figure><\/div>\n\n<div class=\"ub_styled_list \" id=\"ub_styled_list-25b119c9-825a-4278-9e9f-bf95271520d5\"><ul class=\"fa-ul\"><li>Alle Neukunden k\u00f6nnen bei GGBet von einem 375% Wettbonus von bis zu 1500\u20ac profitieren<\/li><li>Wettscheine sind auf mehr als 40 Sportarten zu platzieren<\/li><li>Lizenziert wird GGbet von der Gl\u00fccksspielbeh\u00f6rde auf Curacao<\/li><\/ul><\/div>\n\n\n<link rel='stylesheet' id='my_css-css' href='\/wp-content\/plugins\/hpmtable\/css\/style.css' type='text\/css' media='all' \/>\n\n<style>\n.brand {\n    position: relative;\n}\n\n.brand img {\n    width: 200px;\n    height: 80px;\n}\n\n.casino_field_1 {\n    position: absol